{
    "ComputeServers": {
        "GCP": {
            "FaaSType": "GoogleCloud",
            "Namespace": "faasr-dev",
            "Region": "us-central1",
            "Endpoint": "https://run.googleapis.com/v2/projects/",
            "UseSecretStore": true,
            "ClientEmail": "faasr-workflow-runner@faasr-dev.iam.gserviceaccount.com",
            "TokenUri": "https://oauth2.googleapis.com/token",
            "CPUsPerTask": 1,
            "Memory": 512,
            "TimeLimit": 3600
        },
            "My_SLURM_Server": {
          "FaaSType": "SLURM",
          "Endpoint": "http://slurm.faasr.io:6820",
          "APIVersion": "v0.0.37",
          "Partition": "faasr",
          "Nodes": 1,
          "Tasks": 1,
          "CPUsPerTask": 2,
          "UserName" : "ubuntu",
          "Memory": 2048,
          "TimeLimit": 30,
          "WorkingDirectory": "/tmp"
        },
        "GH": {
        "FaaSType": "GitHubActions",
        "UseSecretStore": true,
        "UserName": "Ashish-Ramrakhiani",
        "ActionRepoName": "FaaSr-workflow-public"
    }
    },
    "DataStores": {
    "My_S3_Bucket": {
      "Endpoint": "https://play.min.io",
      "Bucket": "faasr",
      "Region": "us-east-1"
    }
    },
    "ActionList": {
        "rfunc": {
            "FunctionName": "r_log",
            "FaaSServer": "GCP",
            "Type": "R",
            "Arguments": {
                "message": "testing testing testing"
            },
            "InvokeNext": ["rfunc2"]
        },
         "rfunc2": {
            "FunctionName": "r_log",
            "FaaSServer": "My_SLURM_Server",
            "Type": "R",
            "Arguments": {
                "message": "testing testing testing"
            },
            "InvokeNext": []
        }
    },
    "ActionContainers": {
        "rfunc": "ramrakha/gcp-r:dev4",
        "rfunc2": "ramrakha/slurm-r:dev"
    },
    "FunctionCRANPackage": {
        "r_log": [
            "gapminder"
        ]
    },
    "FunctionGitRepo": {
        "r_log": "nolcut/test-py"
    },
    "FunctionInvoke": "rfunc",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_S3_Bucket",
    "DefaultDataStore": "My_S3_Bucket",
    "InvocationID":"1234",
    "InvocationIDFromDate": "%Y%m%d%H%M",
    "WorkflowName": "test-gcp"
}
