{
  "ActionList": {
    "start": {
      "Arguments": {
        "folder": "tutorialNRPS3",
        "output1": "sample1.csv",
        "output2": "sample2.csv"
      },
      "InvokeNext": [
        "sum"
      ],
      "FaaSServer": "GH",
      "Type": "R",
      "FunctionName": "create_sample_data"
    },
    "sum": {
      "FunctionName": "compute_sum",
      "FaaSServer": "GH",
      "Type": "R",
